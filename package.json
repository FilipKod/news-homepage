{
  "name": "news-homepage",
  "version": "1.0.0",
  "description": "Frontend Mentor - News homepage solution",
  "main": "index.js",
  "scripts": {
    "sass:dev": "sass --watch --style=expanded ./scss/main.scss ./css/styles.css",
    "prefix": "postcss ./css/styles.css --use autoprefixer -d ./css/",
    "sass:prod": "sass --no-source-map --style=compressed scss/main.scss css/styles.css & npm run prefix"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "devDependencies": {
    "autoprefixer": "^10.4.13",
    "postcss": "^8.4.21",
    "postcss-cli": "^10.1.0",
    "sass": "^1.56.1"
  }
}
